{
  "expo": {
    "name": "Reffect",
    "slug": "reffect",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "reffect",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#050608",
      "bundleIdentifier": "com.ado0810.reffect"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#050608"
      },
      "backgroundColor": "#050608",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.ado0810.reffect"
    },
    "web": {
      "output": "static",
      "backgroundColor": "#050608",
      "themeColor": "#050608"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#050608",
          "dark": {
            "backgroundColor": "#050608"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos so you can import images for your boards.",
          "cameraPermission": "The app needs access to your camera so you can capture references directly."
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
